<h1>Pages#home</h1>
<p>Find me in app/views/pages/home.html.erb</p>


<table>
  <thead>
    <tr>
      <th>Name</th>
    </tr>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= link_to(user.first_name, user_path(user)) %> <%= link_to(user.last_name, user_path(user)) %> </td>
      <td><%= image_tag user.avatar.url(:thumb) %></td>
    </tr>
  </tbody>
  <% end %>
</table>
